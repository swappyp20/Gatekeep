# Google OAuth credentials (required)
GOOGLE_OAUTH_CREDENTIALS=./gcp-oauth.keys.json

# Gatekeep security config
GATEKEEP_READ_ONLY=true
GATEKEEP_RISK_THRESHOLD_SUSPICIOUS=0.30
GATEKEEP_RISK_THRESHOLD_DANGEROUS=0.60
GATEKEEP_RISK_THRESHOLD_CRITICAL=0.85

# Threat intelligence (optional)
GATEKEEP_THREAT_INTEL=false
GATEKEEP_THREAT_INTEL_URL=https://api.gatekeep.dev/v1

# Logging
GATEKEEP_LOG_LEVEL=info
GATEKEEP_AUDIT_ENABLED=true
