# Google OAuth credentials (required)
GOOGLE_OAUTH_CREDENTIALS=./gcp-oauth.keys.json

# CalGuard security config
CALGUARD_READ_ONLY=true
CALGUARD_RISK_THRESHOLD_SUSPICIOUS=0.30
CALGUARD_RISK_THRESHOLD_DANGEROUS=0.60
CALGUARD_RISK_THRESHOLD_CRITICAL=0.85

# Threat intelligence (optional)
CALGUARD_THREAT_INTEL=false
CALGUARD_THREAT_INTEL_URL=https://api.calguard.dev/v1

# Logging
CALGUARD_LOG_LEVEL=info
CALGUARD_AUDIT_ENABLED=true
