{
  "name": "gatekeep",
  "version": "0.1.0",
  "description": "MCP security proxy protecting Claude Desktop from calendar-based prompt injection attacks",
  "type": "module",
  "main": "build/index.js",
  "bin": {
    "gatekeep": "build/index.js"
  },
  "files": [
    "build/",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "mcp",
    "claude",
    "security",
    "prompt-injection",
    "google-calendar",
    "gatekeep"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swappyp20/Gatekeep.git"
  },
  "author": "swappyp20",
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:payloads": "vitest run test/unit/security/",
    "test:integration": "vitest run test/integration/",
    "lint": "tsc --noEmit",
    "auth": "node build/index.js auth"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "gaxios": "^6.7.1",
    "google-auth-library": "^9.15.0",
    "googleapis": "^144.0.0",
    "open": "^7.4.2",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@vitest/coverage-v8": "^3.1.1",
    "tsx": "^4.19.0",
    "typescript": "^5.3.3",
    "vitest": "^3.1.1"
  }
}
